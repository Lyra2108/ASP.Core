@model Customer

@{
    ViewBag.Title = "Customer - Detail";
    Layout = "_Layout";
}

<h2>@Model.Name</h2>

<table class="table table-bordered">
    <tr>
        <th>Membership type</th>
        <td>@Model.MembershipType.Name</td>
    </tr>
    <tr>
        <th>Birthdate</th>
        <td>@Model.Birthdate</td>
    </tr>
</table>
